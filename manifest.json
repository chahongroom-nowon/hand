{
  "manifest_version": 3,
  "name": "Handsos Modification",
  "version": "1.0.0",
  "description": "Handsos 웹 확장 프로그램",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "dist/index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["dist/main.js"]
    }
  ]
} 